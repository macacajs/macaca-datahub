{
  "name": "macaca-datahub",
  "version": "1.0.27",
  "description": "Continuous data provider for development, testing, staging and production.",
  "bin": {
    "datahub": "./bin/datahub.js"
  },
  "main": "./index.js",
  "dependencies": {
    "commander": "^2.12.2",
    "datahub-view": "^1.0.8",
    "egg": "^2.0.0",
    "egg-scripts": "^2.4.0",
    "egg-sequelize": "^3.1.0",
    "koa-compose": "^4.0.0",
    "mz": "^2.7.0",
    "socket.io": "^2.0.4",
    "sqlite3": "^3.1.13",
    "xutil": "^1.0.7"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.6",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.13.1",
    "eslint": "^4.12.1",
    "eslint-config-egg": "^5.1.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "view": "npx datahub-view",
    "dev": "npm run view & egg-bin dev",
    "dev:local": "./bin/datahub.js server -c ./macaca-datahub.config.js --verbose",
    "debug": "npm run view & egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint . --fix",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/macacajs/macaca-datahub.git"
  },
  "homepage": "https://macacajs.github.io/macaca-datahub",
  "license": "MIT"
}
